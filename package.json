{
  "name": "jotrip",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:lan": "vite --host --port 5173",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "docker:build": "npm run build && docker build -t jotrip:local .",
    "docker:run": "docker run -p 4000:4000 -v $(pwd)/data:/data jotrip:local"
  },
  "dependencies": {
    "dexie": "^4.2.1",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
}
